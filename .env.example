# Polymarket API credentials
POLY_API_KEY=
POLY_SECRET=
POLY_PASSPHRASE=
POLY_WALLET_ADDRESS=
POLYMARKET_WALLET_PRIVATE_KEY=0x...
POLYMARKET_FUNDER_ADDRESS=

# Polymarket signature type: 0=EOA, 1=POLY_PROXY, 2=POLY_GNOSIS_SAFE
# - Use 0 (EOA) ONLY if your signer address IS the funder address (same wallet)
# - Use 1 (POLY_PROXY) if you use a Polymarket Proxy Wallet
# - Use 2 (POLY_GNOSIS_SAFE) for Gnosis Safe wallets (most common for new users)
# Default: 2 (POLY_GNOSIS_SAFE)
POLYMARKET_SIGNATURE_TYPE=2

# Kalshi API credentials
KALSHI_API_KEY=
KALSHI_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----
...
-----END RSA PRIVATE KEY-----"

# Bot settings
DRY_RUN=true
LOG_LEVEL=info
